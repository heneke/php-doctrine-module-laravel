{
    "name": "hhit/doctrine-module-laravel",
    "description": "Doctrine Module Laravel Integration",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "hhit/doctrine-module": "dev-master",
        "laravel/framework": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.6"
    },
    "authors": [
        {
            "name": "Hendrik Heneke",
            "email": "heneke@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "HHIT\\Doctrine\\Illuminate\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HHIT\\Doctrine\\": "src/test/php"
        }
    },
    "scripts": {
        "prepare-source": [
            "licenser run src mit --replace-existing --owners \"Hendrik Heneke\"",
            "php-cs-fixer fix"
        ]
    }
}
